{
    "manifest_version": 3,
    "name": "Cibus Budget",
    "version": "1.0.0",
    "description": "Calculate Cibus budget for the rest of the month.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
        "cookies",
        "storage"
    ],
    "host_permissions": [
        "https://www.mysodexo.co.il/*"
    ]
}